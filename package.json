    "@types/node": "^14.14.25",
    "@types/sequelize": "^4.28.10",
    "@typescript-eslint/eslint-plugin": "^5.6.0",
    "@typescript-eslint/parser": "^5.6.0",
    "commitizen": "^4.2.4",
    "commitlint": "^13.1.0",
    "concurrently": "^6.4.0",
    "css-loader": "^6.2.0",
    "cz-conventional-changelog": "^3.3.0",
    "eslint": "^8.4.1",
