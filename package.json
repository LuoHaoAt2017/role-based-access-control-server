{
  "name": "role-based-access-control-server",
  "version": "1.0.1",
  "description": "基于角色的访问控制",
  "main": "index.js",
  "scripts": {
    "start": "tsc && concurrently \"npm run watch:ts\" \"npm run serve\"",
    "build:ts": "npm run clean && tsc",
    "watch:ts": "tsc -w",
    "clean": "rimraf ./build",
    "serve": "nodemon --exec babel-node build/index.js",
    "test": "jest --colors --watch",
    "commit": "git add . && git-cz",
    "publish": "git pull && npm version patch && git push"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/LuoHaoAt2017/role-based-access-control-server.git"
  },
  "devDependencies": {
    "@babel/cli": "^7.16.0",
    "@babel/core": "^7.16.5",
    "@babel/node": "^7.16.5",
    "@babel/preset-env": "^7.16.5",
    "@commitlint/cli": "^13.1.0",
    "@commitlint/config-conventional": "^13.1.0",
    "@types/cookie-session": "^2.0.43",
    "@types/cors": "^2.8.12",
    "@types/express": "^4.17.13",
    "@types/jest": "^27.0.2",
    "@types/js-cookie": "^3.0.1",
    "@types/node": "^14.14.25",
    "@types/sequelize": "^4.28.10",
    "@types/superagent": "^4.1.13",
    "@typescript-eslint/eslint-plugin": "^5.6.0",
    "@typescript-eslint/parser": "^5.6.0",
    "commitizen": "^4.2.4",
    "commitlint": "^13.1.0",
    "concurrently": "^6.4.0",
    "css-loader": "^6.2.0",
    "cz-conventional-changelog": "^3.3.0",
    "eslint": "^8.4.1",
    "husky": "^7.0.1",
    "jest": "^27.4.4",
    "mockjs": "^1.1.0",
    "nodemon": "^2.0.15",
    "npm-run-all": "^4.1.5",
    "ts-jest": "^27.1.1",
    "ts-loader": "^8.0.15",
    "ts-node": "^9.1.1",
    "typescript": "^4.1.3"
  },
  "dependencies": {
    "body-parser": "^1.19.1",
    "casbin": "^5.11.5",
    "cheerio": "^1.0.0-rc.10",
    "cookie-session": "^1.4.0",
    "cors": "^2.8.5",
    "express": "^4.17.1",
    "js-cookie": "^3.0.1",
